{
    "manifest_version": 3,
    "name": "qtex Focus",
    "version": "1.0.0",
    "description": "Auto-focus and reload PDF tabs for qtex CLI",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "http://localhost:4848/*"
    ],
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    }
}